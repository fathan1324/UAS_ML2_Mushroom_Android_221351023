<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_nav"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.uas_ml2_221351023.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/datasetFragment"
        android:name="com.example.uas_ml2_221351023.DatasetFragment"
        android:label="Dataset"
        tools:layout="@layout/fragment_dataset" />
    <fragment
        android:id="@+id/featuresFragment"
        android:name="com.example.uas_ml2_221351023.FeaturesFragment"
        android:label="Features"
        tools:layout="@layout/fragment_features" />
    <fragment
        android:id="@+id/arsitekturFragment"
        android:name="com.example.uas_ml2_221351023.ArsitekturFragment"
        android:label="Arsitektur"
        tools:layout="@layout/fragment_arsitektur" />
    <fragment
        android:id="@+id/modelSimulationFragment"
        android:name="com.example.uas_ml2_221351023.ModelSimulationFragment"
        android:label="Simulasi Model"
        tools:layout="@layout/fragment_model_simulation" />
    <fragment
        android:id="@+id/modelFragment"
        android:name="com.example.uas_ml2_221351023.ModelFragment"
        android:label="Model"
        tools:layout="@layout/fragment_model" />

    <action
        android:id="@+id/action_modelFragment_to_arsitekturFragment"
        app:destination="@id/arsitekturFragment" />
    <action
        android:id="@+id/action_modelFragment_to_modelSimulationFragment"
        app:destination="@id/modelSimulationFragment" />
</navigation>